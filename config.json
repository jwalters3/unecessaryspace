{
  "ignore": [
    "**.unpub"
  ],
  "locals": {
    "url": "http://unnecessary.space",
    "name": "Unnecessary Space",
    "owner": "Jeff Walters",
    "description": "Random terrible thoughts on everything."
  },
  "plugins": [
    "./plugins/paginator.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "filenameTemplate": "/:year/:month/:day/:file.html"
}
